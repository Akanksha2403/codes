sets = set()
def sedmax(arr, len):
    if(len == 1):
        return None
    sets.add(max(arr)-min(arr))
    sedmax(arr[1:], len-1
    sedmax(arr[:len-1], len-1)


arr=[5, 1, 4]
sedmax(arr, 3)
print(len(sets)-1)
